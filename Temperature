#include <iostream>

using namespace std;

float Farenheit, Celsius, Kelvin ;

int main () {
    cout << "Tell me the Farenheit Degrees: " ;
    cin >> Farenheit;
    Celsius= (5.0/9.0)*(Farenheit-32) ;
    cout << "Celsius: " << Celsius << endl;
    
    if (Celsius>=100) {
        cout<<"The water boils at this temperature (under typical conditions)";
    }
    else{
        cout<<"The water does not boil(under typical conditions)";
        
    }
    
}
